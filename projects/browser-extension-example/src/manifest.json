{
    "name": "测试插件",
    "description": "测试插件!",
    "version": "1.0",
    "manifest_version": 3,
    "content_security_policy": {
        "content_security_policy": "script-src 'self'; 'unsafe-eval'; object-src 'self';"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "contextMenus",
        "webRequest",
        "declarativeNetRequest"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["./content.js"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "./background.js"
    }
}
